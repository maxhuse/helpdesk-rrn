  "description": "HelpDesk built with React/Redux",
  "main": "server/index.js",
  "scripts": {
    "init": "sqlite3 sql/_helpdesk.sqlite < sql/dump.sql",
    "start": "NODE_ENV=development nodemon --watch server server/index.js",
    "watch": "npm run clean && NODE_ENV=development webpack --watch --progress",
    "build:production": "npm run clean && NODE_ENV=production webpack --progress",
    "clean": "rimraf public",
